{
  "name": "bit-apps",
  "version": "1.0.0",
  "license": "ISC",
  "author": "bit-apps",
  "scripts": {
    "clean": "rimraf node_modules yarn.lock package-lock.json pnpm-lock.yaml",
    "clear-lint-cache": "eslint . --cache --cache-file eslint-cache/eslintcache",
    "dev": "vite",
    "i18n:parse-js": "npx react-gettext-parser --output ./locale.pot --config ./gettext-parser.config.js  './frontend/src/**/{*.ts,*.tsx}'",
    "i18n:json": "[ ! -d \"languages/\" ] && mkdir languages/;npx po2json locale.pot languages/bit-smtp-index-MODULE.json -f jed1.x",
    "i18n:php": "[ ! -d \"languages/\" ] && mkdir languages/;node ./bin/pot-to-php.js locale.pot languages/generatedString.php bit-smtp;wp i18n make-pot . --include='*.php' --skip-js --slug=bit-smtp --headers='{\"Last-Translator\":\"developer@bitcode.pro\",\"Language-Team\":\"support@bitapps.pro\",\"PO-Revision-Date\":\"\"}'",
    "lint": "eslint --ext .jsx --ext .js --ext .ts --ext .tsx ./frontend/src/ --fix",
    "lint:css": "stylelint frontend/**/*.{css,scss} --aei --color --fix",
    "lint:php": "composer lint",
    "compat": "composer compat",
    "preview": "vite preview",
    "test": "vitest --watch=false",
    "test:watch": "vitest --watch=true",
    "test:cover": "vitest --coverage",
    "test:php": "composer test:unit",
    "build": "tsc && vite build",
    "pretty": "npx prettier ./frontend/src --check",
    "pretty:fix": "npx prettier ./frontend/src --check --write",
    "pretty:quick": "pretty-quick --staged",
    "ts-check": "tsc",
    "pre-commit": "lefthook run pre-commit",
    "sb": "storybook dev -p 6006",
    "all:test": "pnpm ts && pnpm lint",
    "production": "composer i --no-dev && pnpm install && pnpm build && pnpm i18n:parse-js && pnpm i18n:php"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.21.1",
    "@ant-design/icons": "^5.4.0",
    "@emotion/react": "^11.13.3",
    "@loadable/component": "^5.16.4",
    "@tanstack/react-query": "^4.36.1",
    "@tanstack/react-query-devtools": "^4.36.1",
    "@tippyjs/react": "^4.2.6",
    "@yaireo/tagify": "^4.31.3",
    "antd": "^5.20.6",
    "framer-motion": "^10.18.0",
    "immer": "^10.1.1",
    "jotai": "^2.9.3",
    "localforage": "^1.10.0",
    "lucide-react": "^0.190.0",
    "react": "^18.3.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.3.1",
    "react-draggable": "^4.4.6",
    "react-focus-bounder": "^1.1.6",
    "react-hot-toast": "^2.4.1",
    "react-multiple-select-dropdown-lite": "^2.0.6",
    "react-router-dom": "^6.26.2",
    "simplebar-react": "^3.2.6",
    "tippy.js": "^6.3.7",
    "use-immer": "^0.9.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@emotion/babel-plugin": "^11.12.0",
    "@ls-lint/ls-lint": "^1.11.2",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@total-typescript/ts-reset": "^0.4.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/css-modules": "^1.0.5",
    "@types/jest": "^29.5.13",
    "@types/loadable__component": "^5.13.9",
    "@types/react": "^18.3.7",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "^18.3.0",
    "@types/yaireo__tagify": "^4.27.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-c8": "^0.30.1",
    "@vitest/ui": "^0.30.1",
    "@wordpress/i18n": "^4.58.0",
    "autoprefixer": "^10.4.20",
    "conventional-changelog-atom": "^2.0.8",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
